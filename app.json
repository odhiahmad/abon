{
  "expo": {
    "userInterfaceStyle": "automatic",
    "name": "AbsensiOnline",
    "slug": "AbsensiOnline",
    "version": "3.4.3",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    
    "ios": {
      "infoPlist": {
        "CFBundleAllowMixedLocalizations": true,
        "NSCameraUsageDescription": "Kamera digunakan untuk upload foto jika absen diluar kantor",
        "NSLocationWhenInUseUsageDescription": "Lokasi digunakan untuk mengambil posisi anda terkini untuk mencek lokasi anda berada didekat kantor atau tidak"
      },
      "supportsTablet": true,
      "bundleIdentifier": "COM.KOMINFOSUMBAR.ABONSUMBAR"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "android": {
      "package": "COM.KOMINFOSUMBAR.ABONSUMBAR"
    }
  }
}